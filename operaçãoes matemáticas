programa {
  funcao inicio() {
    
    //operaçãoes matemáticas
    inteiro valor1, valor2

    escreva("Digite o valor 1: ")
    leia(valor1)
    escreva("Digite o valor de 2")
    leia(valor2)


escreva("O resultado das 4 operações: ")
escreva("\nSoma: ", valor1 + valor2)
escreva("\nSubtração: ", valor1 - valor2)
escreva("\nMultiplicação: ", valor1 * valor2)
escreva("\nDivisão: ", valor1 / valor2)

  }
}
